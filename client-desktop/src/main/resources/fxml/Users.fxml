<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<?import org.kordamp.ikonli.javafx.FontIcon?>

<BorderPane prefHeight="600.0"
            prefWidth="1000.0"
            maxHeight="-Infinity"
            maxWidth="-Infinity"
            stylesheets="@../styles/Styles.css"
            xmlns="http://javafx.com/javafx/12.0.2"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.projeto.integrador.clientdesktop.controllers.UsersController">

  <left>
    <VBox prefHeight="600.0"
      prefWidth="100.0"
      spacing="0.0"
      alignment="TOP_CENTER"
      styleClass="sidebar-menu">

      <children>
        <Button onAction="#goToDashboard" text="Dashboard" textAlignment="CENTER" contentDisplay="TOP">
          <graphic>
            <FontIcon iconLiteral="fas-project-diagram" iconSize="20"/>
          </graphic>
        </Button>
      </children>
    </VBox>
  </left>

  <center>
    <AnchorPane fx:id="rootAnchorPane" prefHeight="600.0" prefWidth="900.0" maxHeight="-Infinity" maxWidth="-Infinity">
      <children>
      </children>
    </AnchorPane>
  </center>

</BorderPane>
